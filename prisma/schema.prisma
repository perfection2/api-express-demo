// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./dev.db"
}

model UserModel {
  id       Int           @id @default(autoincrement())
  email    String
  password String
  name     String
  tickets  TicketModel[]
}

model TicketModel {
  id          Int       @id @default(autoincrement())
  name        String
  description String
  deadline    DateTime
  assignee    UserModel @relation(fields: [assigneeId], references: [id])
  assigneeId  Int
}
